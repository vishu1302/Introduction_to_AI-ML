    "import numpy as np\n",
    "import math\n",
    "\n",
    "#defining vectors\n",
    "P = np.array([np.cos(math.pi/2) , np.sin(math.pi/2)])\n",
    "Q = np.array([3*np.cos(math.pi/6) , 3*np.sin(math.pi/6)])\n",
    "\n",
    "#function for finding distance\n",
    "def distance(a):\n",
    "    return a*math.sqrt(np.square(P-Q).sum())\n",
    "Dist=distance(5)\n",
    "print(Dist)\n"
   ]
